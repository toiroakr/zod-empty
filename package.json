{
  "name": "zod-empty",
  "version": "1.2.2",
  "description": "generate minimum data from zod schema.",
  "types": "./cjs/index.d.ts",
  "main": "./cjs/index.cjs",
  "module": "./esm/index.js",
  "scripts": {
    "prepare": "is-ci || husky install",
    "build": "packemon pack --addEngines --addExports --declaration",
    "release": "npm run build && npm publish",
    "check": "biome check --write",
    "check:force": "biome check --write --unsafe",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toiroakr/zod-empty.git"
  },
  "keywords": [
    "zod",
    "empty",
    "schema",
    "react-hook-form"
  ],
  "author": "toiroakr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toiroakr/zod-empty/issues"
  },
  "homepage": "https://github.com/toiroakr/zod-empty#readme",
  "dependencies": {
    "just-clone": "6.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "14.0.2",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "2.1.8",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "lint-staged": "13.0.3",
    "packemon": "4.1.1",
    "semantic-release": "^24.2.0",
    "typescript": "5.7.2",
    "vitest": "2.1.8",
    "vitest-github-actions-reporter": "0.11.1",
    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "typescript": "5.x",
    "zod": "3.x"
  },
  "packemon": [
    {
      "inputs": {
        "index": "./src/index.ts"
      },
      "platform": "node",
      "format": "cjs",
      "support": "current"
    },
    {
      "inputs": {
        "index": "./src/index.ts"
      },
      "platform": "browser",
      "support": "current"
    }
  ],
  "files": [
    "cjs/**/*",
    "cjs/**/*.{cjs,mjs,map}",
    "dts/**/*.d.ts",
    "esm/**/*",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "mjs/**/*.{mjs,map}",
    "src/**/*",
    "src/**/*.{ts,tsx,json}"
  ],
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=8.1.0"
  },
  "volta": {
    "node": "16.16.0",
    "npm": "8.16.0"
  },
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "node": {
        "import": "./mjs/*.mjs",
        "require": "./cjs/*.cjs"
      }
    },
    ".": {
      "node": {
        "types": "./cjs/index.d.ts",
        "import": "./cjs/index-wrapper.mjs",
        "default": "./cjs/index.cjs"
      },
      "default": {
        "types": "./esm/index.d.ts",
        "import": "./esm/index.js"
      }
    }
  }
}
