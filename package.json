{
  "name": "zod-empty",
  "version": "1.4.3",
  "description": "generate minimum data from zod schema.",
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "scripts": {
    "prepare": "lefthook install",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "release": "npm run build && npm publish",
    "check": "biome check --write",
    "check:force": "biome check --write --unsafe",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toiroakr/zod-empty.git"
  },
  "keywords": [
    "zod",
    "empty",
    "schema",
    "react-hook-form"
  ],
  "author": "toiroakr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toiroakr/zod-empty/issues"
  },
  "homepage": "https://github.com/toiroakr/zod-empty#readme",
  "dependencies": {
    "just-clone": "6.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "2.1.8",
    "lefthook": "^1.10.1",
    "pkg-pr-new": "^0.0.41",
    "semantic-release": "^24.2.1",
    "typescript": "5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "2.1.8",
    "vitest-github-actions-reporter": "0.11.1",
    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "typescript": "5.x",
    "zod": "3.x"
  },
  "files": [
    "dist/**/*",
    "!src/index.spec.ts",
    "src/index.ts"
  ],
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=8.1.0"
  },
  "volta": {
    "node": "16.16.0",
    "npm": "8.16.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "types": "./dist/mjs/index.d.ts",
        "import": "./dist/mjs/index.js",
        "default": "./dist/cjs/index.js"
      },
      "default": {
        "types": "./dist/esm/index.d.ts",
        "import": "./dist/esm/index.js"
      }
    }
  }
}
